{
  "name": "nuxtlify",
  "version": "1.0.0",
  "description": "Blog template for Nuxt.js/Netlify",
  "author": "Daniel Kelly",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "before-build": "node -r esm ./build-tools/beforeBuild.js",
    "after-build": "node -r esm ./build-tools/afterBuild.js",
    "dev": "npm run before-build && nuxt -o && npm run after-build",
    "build": "npm run before-build && nuxt build && npm run after-build",
    "start": "nuxt start",
    "generate": "npm run before-build && nuxt generate && npm run after-build",
    "local-cms": "npx netlify-cms-proxy-server"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/axios": "^5.12.3",
    "@nuxtjs/google-gtag": "^1.0.4",
    "animate.css": "4.*",
    "axios": "^0.21.0",
    "esm": "^3.2.25",
    "gray-matter": "^4.0.2",
    "highlight.js": "10.*",
    "html-loader": "1.3.2",
    "intersection-observer": "^0.11.0",
    "lodash.chunk": "^4.2.0",
    "lodash.isstring": "^4.0.1",
    "lodash.range": "^3.2.0",
    "markdown-it": "12.*",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "md5": "2.*",
    "ncp": "^2.0.0",
    "netlify-cms": "^2.9.5",
    "nuxt": "^2.14.9",
    "nuxt-buefy": "^0.4.4",
    "nuxt-fontawesome": "^0.4.0",
    "opti-image": "^0.10.0",
    "react": "^16.8.6",
    "showdown": "^1.9.1",
    "url-parse": "^1.4.7",
    "v-runtime-template": "^1.10.0",
    "vue-disqus": "4.*"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@nuxt/types": "^2.14.9",
    "@nuxtjs/eslint-config": "5.*",
    "@nuxtjs/eslint-module": "3.*",
    "@nuxtjs/style-resources": "1.*",
    "babel-eslint": "10.*",
    "eslint": "7.*",
    "eslint-config-prettier": "^6.*",
    "eslint-config-standard": "16.*",
    "eslint-plugin-import": "2.*",
    "eslint-plugin-jest": "24.*",
    "eslint-plugin-node": "11.*",
    "eslint-plugin-nuxt": "2.*",
    "eslint-plugin-prettier": "3.*",
    "eslint-plugin-promise": "4.*",
    "eslint-plugin-vue": "7.*",
    "gray-matter-loader": "0.0.10",
    "node-sass": "5.*",
    "nodemon": "2.*",
    "nuxt-responsive-loader": "^2.0.1",
    "prettier": "2.*",
    "rimraf": "3.*",
    "sass-loader": "10.*",
    "sharp": "^0.26.3",
    "vue": "^2.6.12",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  }
}
